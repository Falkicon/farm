{
  "name": "fluent-fabric-migration-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run lint && npm run test && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:experiments": "vite build --config vite.experiments.config.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "postbuild": "npm run size-check",
    "size-check": "node scripts/check-bundle-size.js"
  },
  "// IMPORTANT": "Do not change these specific versions - they are required for compatibility testing",
  "dependencies": {
    "@fabric-msft/fabric-web": "2.0.0-beta.4",
    "@fabric-msft/theme": "1.0.0-beta",
    "@fluentui/web-components": "3.0.0-beta.74",
    "@microsoft/fast-element": "2.0.0-beta.26"
  },
  "devDependencies": {
    "@open-wc/testing": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitest/coverage-v8": "^2.1.8",
    "chalk": "^5.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^15.11.7",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
